{
	"name": "@solid-cli/create",
	"version": "0.6.0-alpha.0",
	"description": "Create Solid apps with low configuration",
	"author": "Thomas Beer",
	"contributors": [
		"Rahul Batra"
	],
	"license": "MIT",
	"homepage": "https://solid-cli.netlify.app",
	"repository": {
		"type": "git",
		"url": "https://github.com/solidjs-community/solid-cli"
	},
	"keywords": [
		"solidjs",
		"solid",
		"cli"
	],
	"main": "./dist/index.mjs",
	"module": "./dist/index.mjs",
