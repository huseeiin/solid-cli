		"tsup": "^8.3.5"
	},
	"publishConfig": {
		"access": "public"
	},
	"dependencies": {
		"@clack/prompts": "0.9.1",
		"picocolors": "^1.1.1",
		"@begit/core": "^0.1.1",
		"citty": "^0.1.6",
		"sucrase": "^3.35.0",
		"@solid-cli/utils": "workspace:*"
